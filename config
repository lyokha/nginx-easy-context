ngx_addon_name=ngx_easy_context
ngx_easy_context_dir=$ngx_addon_dir

ngx_module_type=MISC
ngx_module_name=$ngx_addon_name

NGX_HTTP_LOG_PLUGIN_DEPS="$ngx_addon_dir/$ngx_addon_name.h"
NGX_HTTP_LOG_PLUGIN_SRCS="$ngx_addon_dir/$ngx_addon_name.c"
ngx_module_srcs="$NGX_HTTP_LOG_PLUGIN_SRCS"
ngx_module_deps="$NGX_HTTP_LOG_PLUGIN_DEPS"

. auto/module

